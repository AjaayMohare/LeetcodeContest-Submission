class Solution {
public:
    int minOperations(vector<int>& nums) {
        map<int,int> m;
        for(int i=nums.size()-1;i>=0;i--){
            if(m.count(nums[i])){
                int re =nums.size()-m.size();
                if(re%3){
                    return 1 + (re/3);
                }
                return re/3;
            }
            else{
                m[nums[i]]++;
            }
        }
        return 0;
    }
};
